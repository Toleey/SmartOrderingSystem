page {
  background-color: #F4F2F9;
}
.box {
  margin: 30rpx auto;
  min-height: 300rpx;
  width: 700rpx;
  background-color: white;
}
.box .box01 {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 140rpx;
}
.box .box01 .fetchMealCode-title {
  font-size: 40rpx;
}
.box .box01 .fetchMealCode-content {
  font-size: 80rpx;
  font-weight: bold;
  color: red;
}
.box .buyItems {
  height: 50rpx;
}
.box .box02 {
  height: 50rpx;
}
.box .box03 {
  height: 50rpx;
}
.box .box04 {
  display: flex;
  justify-content: flex-end;
  height: 60rpx;
}
.box .box04 .contactShop {
  border: 1px solid black;
  cursor: pointer;
}
.box .box04 .requestFund {
  border: 1px solid black;
  cursor: pointer;
  margin-left: 20rpx;
}

